{
  "name": "Flow Documentation - Cadence Reference Outline",
  "version": "1.0",
  "description": "Extension scaffold",

  "permissions": ["activeTab", "declarativeContent"],

  "page_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://docs.onflow.org/docs/cadence"],
      "js": ["contentScript.js"],
      "css": ["reference-list.css"]
    }
  ],

  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
